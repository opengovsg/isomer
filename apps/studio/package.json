{
  "name": "isomer-studio",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "generate": "prisma generate && tsx prisma/generate.cts && eslint --fix prisma/generated && bunx prettier --write prisma/generated",
    "generate:jwks": "tsx scripts/generateJwks.ts",
    "services:setup": "docker compose -f \"docker-compose.yml\" up -d --wait",
    "db:sleep": "sleep 3",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset",
    "db:push": "prisma db push",
    "setup": "run-s --print-label services:setup db:sleep migrate:dev db:seed",
    "setup:test": "run-s --print-label services:setup db:sleep migrate generate",
    "teardown": "docker compose down",
    "predev": "run-s migrate",
    "dev": "next dev",
    "build": "SKIP_ENV_VALIDATION=true next build",
    "build:theme": "bunx chakra-cli tokens src/theme/index.ts",
    "build:preview-tw": "NODE_ENV=production bunx tailwindcss build -i src/styles/tailwind.css -o public/assets/css/preview-tw.css &&  prettier --write public/assets/css/preview-tw.css",
    "prestart": "bun run migrate",
    "start": "next start",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "format:fix": "prettier --write . --ignore-path ../../.gitignore",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "migrate:dev": "prisma migrate dev",
    "migrate:custom": "tsx prisma/custom/install.ts",
    "migrate": "prisma migrate deploy",
    "test": "run-s test:*",
    "test:unit": "dotenv -e .env.test vitest run",
    "test-ci:unit": "dotenv -e .env.test vitest run -- --coverage",
    "test:load:build": "webpack --config tests/load/webpack.config.js",
    "dev:e2e": "dotenv -e .env.test start-server-and-test dev http://127.0.0.1:3000 test:e2e",
    "test:e2e": "dotenv -e .env.test bunx playwright test",
    "test-dev": "start-server-and-test dev http://127.0.0.1:3000 test",
    "test-dev:unit": "dotenv -e .env.test vitest",
    "test-ci:e2e": "dotenv -e .env.test start-server-and-test start http://127.0.0.1:3000 test:e2e",
    "storybook": "storybook dev -p 6007",
    "build-storybook": "storybook build",
    "clean": "git clean -xdf .next .turbo node_modules build",
    "db:connect": "bash ./scripts/connectRds.sh prod",
    "db:connect:staging": "bash ./scripts/connectRds.sh staging",
    "db:connect:uat": "bash ./scripts/connectRds.sh uat",
    "db:connect:vapt": "bash ./scripts/connectRds.sh vapt",
    "migrate:staging": "source .ssh/.env.staging && bunx prisma migrate deploy",
    "migrate:uat": "source .ssh/.env.uat && bunx prisma migrate deploy",
    "test:watch": "dotenv -e .env.test bunx vitest watch"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
